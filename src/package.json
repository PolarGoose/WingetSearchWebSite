{
  "name": "WingetSearchWebSite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "prettier -w . && eslint . && tsc -p create-winget-packages-flexsearch-db/tsconfig.json && npm run db:start && tsc -b web-site/tsconfig.json && vite build --emptyOutDir web-site",
    "web:dev": "vite web-site",
    "db:debug": "node --inspect-brk --loader ts-node/esm create-winget-packages-flexsearch-db/src/index.ts",
    "db:start": "node create-winget-packages-flexsearch-db/src/index.ts"
  },
  "dependencies": {
