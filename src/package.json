    "execa": "^9.6.1",
    "fast-glob": "^3.3.3",
    "flexsearch": "^0.8.212",
    "jiti": "^2.6.1",
    "prettier": "^3.7.4",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "sql.js": "^1.13.0",
    "yaml": "^2.8.2",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
